{
  "compilerOptions": {
    "baseUrl": "./",
    "declaration": true,
    "lib": [
      "es2017",
      "esnext"
    ],
    "moduleResolution": "node",
    "downlevelIteration": true,
    "noFallthroughCasesInSwitch": true,
    "noImplicitReturns": true,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "outDir": "./types",
    "paths": {},
    "rootDir": "./src",
    "strict": true,
    "types": [
      "node",
    ],
    "typeRoots": [
      "node_modules/@types",
    ],
  },
  "include": [
    "src/**/*.ts"
  ],
}